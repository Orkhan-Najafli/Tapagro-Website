{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev --host 127.0.0.1 --port 4572",
    "dev:test": "set NODE_ENV=production && nuxt dev --host 127.0.0.1 --port 4572 --dotenv .env.production",
    "dev:staging": "cross-env NODE_ENV=staging nuxt dev --dotenv .env.staging",
    "dev:production": "cross-env NODE_ENV=production nuxt dev --dotenv .env.production",
    "build:staging": "cross-env NODE_ENV=staging nuxt build --dotenv .env.staging",
    "build:production": "cross-env NODE_ENV=production nuxt build --dotenv .env.production",
    "generate:staging": "cross-env NODE_ENV=staging nuxt generate --dotenv .env.staging",
    "generate:production": "cross-env NODE_ENV=production nuxt generate --dotenv .env.production",
    "pm2-start:staging": "cross-env NODE_ENV=staging nuxt build --dotenv .env.staging && pm2 start .output/server/index.mjs --name tapagro_website --env staging -- start",
    "pm2-restart:staging": "cross-env NODE_ENV=staging nuxt build --dotenv .env.staging && pm2 restart tapagro_website",
    "pm2-start:production": "cross-env NODE_ENV=production nuxt build --dotenv .env.production && pm2 start .output/server/index.mjs --name tapagro_website --env production -- start",
    "pm2-restart:production": "cross-env NODE_ENV=production nuxt build --dotenv .env.production && pm2 restart tapagro_website"
  },
  "dependencies": {
    "@ant-design-vue/nuxt": "^1.4.2",
    "@nuxtjs/i18n": "^8.5.1",
    "@nuxtjs/seo": "^2.0.0-rc.23",
    "@nuxtjs/sitemap": "^6.1.1",
    "@pinia/nuxt": "^0.5.4",
    "@vee-validate/i18n": "^4.14.7",
    "@vee-validate/nuxt": "^4.13.2",
    "@vee-validate/rules": "^4.14.7",
    "@vueuse/core": "^11.2.0",
    "ant-design-vue": "^4.2.3",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "nuxt": "^3.13.0",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "vee-validate": "^4.14.7",
    "vue": "latest",
    "vue-recaptcha-v3": "^2.0.1"
  },
  "overrides": {
    "vue": "latest"
  },
  "devDependencies": {
    "@types/qs": "^6.9.15",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vue-i18n": "^9.14.0"
  }
}
